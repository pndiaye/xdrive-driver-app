{
  "expo": {
    "name": "XDrive Driver App",
    "slug": "xdrive-driver-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xdrive.xdrivedriverapp",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification",
          "location",
          "fetch",
          "remote-notification",
          "location",
          "fetch",
          "remote-notification",
          "location",
          "fetch",
          "remote-notification"
        ],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Cette application utilise votre localisation pour vous connecter avec des clients à proximité",
        "NSLocationWhenInUseUsageDescription": "Cette application utilise votre localisation pour vous connecter avec des clients à proximité",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.xdrive.xdrivedriverapp"
    },
    "web": {
      "favicon": "./src/assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "148107ae-6aa5-4c92-9320-b848b079814c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/148107ae-6aa5-4c92-9320-b848b079814c"
    }
  }
}
